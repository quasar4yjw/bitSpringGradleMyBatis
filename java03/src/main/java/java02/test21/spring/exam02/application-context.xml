<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">


    <!-- 호출할 생성자를 지정하지 않으면 기본 생성자를 호출한다.
    
    -->
    
    <bean id = "b01" class="java02.test21.spring.exam02.Car"></bean>
    
    <!--  빈의 이름(별명) 지정하기
      ID가 없으면 빈의 첫 번째 별명을 ID로 사용한다.
     -->
    <bean name = "b02" class="java02.test21.spring.exam02.Car"></bean>
    
    <!--  빈의 이름(별명)을 여러개 지정하기 -->
    <bean name = "b11,     b12,    b13" class="java02.test21.spring.exam02.Car"></bean>
    <bean name = "b21;    b22;       b23" class="java02.test21.spring.exam02.Car"></bean>
    <bean name = "b03   b04   b05" class="java02.test21.spring.exam02.Car"></bean>
    
    <bean id = "b100"   name ="b101   b102  b103" class="java02.test21.spring.exam02.Car"></bean>

 <bean class="java02.test21.spring.exam02.Car"></bean>
 <bean class="java02.test21.spring.exam02.Car"></bean>
 <bean class="java02.test21.spring.exam02.Car"></bean>
</beans>